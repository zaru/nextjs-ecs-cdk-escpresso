{
	"containerDefinitions": [
		{
			"name": "nextjs",
			"image": "826204725037.dkr.ecr.ap-northeast-1.amazonaws.com/nextjs-develop-repository22e53bbd-sko2jndt10kg:latest",
			"environment": [
				{
					"name": "AWS_REGION",
					"value": "ap-northeast-1"
				},
				{
					"name": "TZ",
					"value": "Asia/Tokyo"
				}
			],
			"essential": true,
			"logConfiguration": {
				"logDriver": "awslogs",
				"options": {
					"awslogs-group": "{{ ssm `/ecs/next-js-cdk/log-group-name` }}",
					"awslogs-region": "ap-northeast-1",
					"awslogs-stream-prefix": "ecs"
				}
			},
			"portMappings": [
				{
					"containerPort": 3000,
					"hostPort": 3000,
					"protocol": "tcp"
				}
			]
		}
	],
	"cpu": "256",
	"executionRoleArn": "{{ ssm `/ecs/next-js-cdk/task-exec-role` }}",
	"family": "next-js-cdk",
	"memory": "512",
	"networkMode": "awsvpc",
	"requiresCompatibilities": ["FARGATE"],
	"taskRoleArn": "{{ ssm `/ecs/next-js-cdk/task-role` }}"
}
